{
    "name": "haxibiao/nova-tools",
    "description": "哈希表扩展后台能力的 nova tools",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "haxibiao/nova": "*",
        "laravel/framework": "^8.27"
    },
    "autoload": {
        "psr-4": {
            "Haxibiao\\Nova\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Haxibiao\\Nova\\SelectAutoCompleteServiceProvider",
                "Haxibiao\\Nova\\CardServiceProvider",
                "Haxibiao\\Nova\\QuestionViewerServiceProvider",
                "Haxibiao\\Nova\\FieldServiceProvider",
                "Haxibiao\\Nova\\EditorServiceProvider",
                "Haxibiao\\Nova\\ShuaziServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
